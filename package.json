{
  "name": "kvdb-mem",
  "module": "index.ts",
  "type": "module",
  "version": "2.0.0",
  "private": true,
  "bin": {
    "kvdb-mem": "./dist/kvdb-mem"
  },
  "scripts": {
    "dev": "bun run --watch ./src/index.ts",
    "mcp": "bun run ./src/mcp/index.ts",
    "dev:mcp": "bun run --watch ./src/mcp/index.ts",
    "dev:all": "bun run --watch ./src/index.ts & bun run --watch ./src/mcp/index.ts",
    "cli": "bun run ./src/cli/index.ts",
    "cli:backup": "bun run ./src/cli/index.ts backup",
    "cli:repair": "bun run ./src/cli/index.ts repair",
    "cli:check": "bun run ./src/cli/index.ts check",
    "build": "bun run ./build.ts",
    "build:cli": "bun run ./build.ts",
    "test:mcp": "bun run ./test-mcp.js",
    "bench": "bun run ./benchmarks/index.ts",
    "bench:kv": "bun run ./benchmarks/kv-performance.ts",
    "bench:links": "bun run ./benchmarks/link-traversal.ts",
    "bench:sqlite-safety": "bun run ./benchmarks/sqlite-crash-safety.ts",
    "deploy:dry-run": "bash ./scripts/migration-dry-run.sh",
    "deploy:verify": "bunx tsc --noEmit && bun test tests/kv.sqlite.test.ts tests/db.schema.test.ts tests/db.migrate.test.ts tests/api-compatibility.test.ts tests/concurrent-access.test.ts tests/final-verification.test.ts",
    "script:backup": "bun run ./scripts/backup-kv-db.ts",
    "script:repair": "bun run ./scripts/repair-fts5-index.ts",
    "script:check": "bun run ./scripts/test-startup-fts5-integrity.ts",
    "commitlint": "commitlint --from HEAD~1 --to HEAD"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.6",
    "@semantic-release/npm": "^13.1.4",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/bun": "latest",
    "semantic-release": "^25.0.3"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@toon-format/toon": "^2.1.0",
    "effect": "^3.19.15",
    "fastmcp": "^3.31.0",
    "zod": "^4.3.6"
  }
}
