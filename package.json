{
  "name": "kvdb-mem",
  "module": "index.ts",
  "type": "module",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "bun run --watch ./src/index.ts",
    "mcp": "bun run ./src/mcp/index.ts",
    "dev:mcp": "bun run --watch ./src/mcp/index.ts",
    "dev:all": "bun run --watch ./src/index.ts & bun run --watch ./src/mcp/index.ts",
    "test:mcp": "bun run ./test-mcp.js",
    "bench": "bun run ./benchmarks/index.ts",
    "bench:kv": "bun run ./benchmarks/kv-performance.ts",
    "bench:links": "bun run ./benchmarks/link-traversal.ts",
    "deploy:dry-run": "bash ./scripts/migration-dry-run.sh",
    "deploy:verify": "bunx tsc --noEmit && bun test tests/kv.sqlite.test.ts tests/db.schema.test.ts tests/db.migrate.test.ts tests/api-compatibility.test.ts tests/concurrent-access.test.ts tests/final-verification.test.ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@toon-format/toon": "^2.1.0",
    "effect": "^3.19.15",
    "fastmcp": "^3.31.0",
    "zod": "^4.3.6"
  }
}
